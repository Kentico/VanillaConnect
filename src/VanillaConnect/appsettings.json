{
  "Logging": {
    "IncludeScopes": false,
    "PathFormat": "Logs/VanillaConnect-{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Intercom": {
    "ApiUri": "https://api.intercom.io/",
    "AppId": "",
    "AccessToken": "",
    "ProfileUrlPropertyName": "forums_member",
    "CachingTimeoutMinutes": 30,
    "PageSize": 60,
    "BurstSize": 50,
    "BurstDelaySeconds": 10
  },
  "Vanilla": {
    "AllowWhitespaceInUsername": "false",
    "AllowAccentsInUsername": "false",
    "AllowDuplicateUserNames": "false",
    "BaseUri": "https://forums.kenticocloud.com/"
  },
  "AuthRocket": {
    "UseIntercom": "true"
  }
}
